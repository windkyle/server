<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "config/mybatis-3-mapper.dtd">
<mapper namespace="mapping.staffMapper">
    <!--卡号查询单个人员-->
    <select id="getSingleStaff" resultType="com.dyw.queue.entity.StaffEntity">
        select * from Staff where CardNumber=#{_parameter }
    </select>
    <!--获取全部人员信息-->
    <select id="getAllStaff" resultType="com.dyw.queue.entity.StaffEntity">
        select * from Staff
    </select>
</mapper>